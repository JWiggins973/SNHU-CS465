<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>Contact - Travlr Getaways Website Template</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			{{> headers}}
			<div id="contents">
				<div class="box">
					<div>
						<div id="contact" class="body">
							<h1>Contact</h1>
							<form action="/index" method="post">
								<table>
									<tbody>
			{{#each contact.textboxField}}
										<tr>
											<td>{{this}}</td>
											<td><input type="text" value="" class="txtfield"></td>
										</tr> 
            {{/each}}
										<tr>
											<td class="txtarea">{{contact.messageBox}}</td>
											<td><textarea></textarea></td>
										</tr> <tr>
											<td></td>
											<td><input type="submit" value="" class="btn"></td>
										</tr>
									</tbody>
								</table>
							</form>
							<h2>Travlr Getaways</h2>
            {{#each contact.contactInfo}}
							<p>
								<span>{{this.label}}</span> {{this.content}}
							</p>
            {{/each}}
						</div>
					</div>
				</div>
			</div>
		</div>
		{{> footer}}
	</div>
</body>
</html>